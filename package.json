{
  "name": "codecommit-github-migrator",
  "type": "module",
  "version": "0.2.3",
  "description": "Helps to migrate a CodeCommit repository to GitHub",
  "author": "Alexej Liebenthal",
  "bin": {
    "cgm": "./bin/run.js"
  },
  "homepage": "https://github.com/AlexejLiebenthal/codecommit-github-migrator",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "AlexejLiebenthal/codecommit-github-migrator",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@aws-sdk/client-codecommit": "^3.282.0",
    "@aws-sdk/client-sts": "^3.289.0",
    "@aws-sdk/credential-providers": "^3.282.0",
    "@oclif/core": "^2.4.0",
    "@oclif/plugin-help": "^5.2.6",
    "@octokit/rest": "^19.0.7",
    "execa": "^7.0.0",
    "inquirer": "^9.1.4"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.8",
    "@tsconfig/node18-strictest-esm": "^1.0.1",
    "@types/chai": "^4.3.4",
    "@types/inquirer": "^9.0.3",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.5",
    "chai": "^4.3.7",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.2.0",
    "oclif": "^3.7.0",
    "prettier": "^2.8.4",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "cgm",
    "dirname": "cgm",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {}
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/AlexejLiebenthal/codecommit-github-migrator/issues",
  "keywords": [
    "oclif",
    "codecommit",
    "github",
    "migration",
    "migrator",
    "cli"
  ],
  "types": "dist/index.d.ts"
}
